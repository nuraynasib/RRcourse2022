library(fizzbuzzR)
library(fizzbuzzR)
fizzbuzz(start = 1, end = 100, mod1 = 3, mod2 = 5)
recurse_fibonacci <- function(n) {
if(n <= 1) {
return(n)
} else {
return(recurse_fibonacci(n-1) + recurse_fibonacci(n-2))
}
}
